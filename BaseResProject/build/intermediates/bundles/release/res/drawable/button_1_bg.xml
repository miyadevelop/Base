<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="?android:attr/textColorHighlightInverse">
    <item>
        <shape>
            <!-- 描边 -->
            <stroke
                android:width="@dimen/button_outline_width"
                android:color="@color/button_1_outline" />
            <!-- 圆角 -->
            <corners android:radius="@dimen/button_corner_radius" />
            <padding
                android:bottom="@dimen/view_padding_1"
                android:left="@dimen/view_padding_1"
                android:right="@dimen/view_padding_1"
                android:top="@dimen/view_padding_1" />
            <!-- 填充 -->
            <solid android:color="?attr/colorButtonNormal" />
        </shape>
    </item>
